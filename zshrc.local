#!/usr/bin/env zsh

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
source ~/.zsh/defaults
source /usr/share/nvm/init-nvm.sh
source ~/.zsh/env
source ~/.zsh/cmds

# interactive-only stuff
[[ $- == *i* ]] || return
source ~/.zsh/theme
source ~/.zsh/completions
source ~/.zsh/aliases
source ~/.zsh/history
source ~/.zsh/prompt

if command -v tmux>/dev/null; then
  [[ ! $TERM =~ screen ]] && [ -z $TMUX ] && exec tmux
fi
