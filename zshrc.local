#!/usr/bin/env zsh

[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
source ~/.zsh/defaults
source ~/.zsh/env
source ~/.zsh/cmds
source /usr/share/nvm/init-nvm.sh

# interactive-only stuff
[[ $- == *i* ]] || return
source ~/.zsh/theme
source ~/.zsh/completions
source ~/.zsh/aliases
source ~/.zsh/history
source ~/.zsh/prompt
